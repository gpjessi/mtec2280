 // Mechanical Garden a Natures Mechanic Bloom
 
size(1080, 720);
background(220, 230, 240);
 
 // Organic elements
fill(120, 200, 100); // Green color
noStroke();
ellipse(540, 360, 300, 300); // Central organic form

// Mechanical elements
strokeWeight(5);
stroke(150, 150, 150); // Silver color for machinery

 // Ellipse 
fill(120,300,50);
ellipse(360, 250, 80, 120);

 // Triangle
fill(200,300,100);
triangle(700, 250, 800, 250, 750, 150);

  // Rectangle
rect(600, 500, 100, 150);

 // Connecting lines
line(540, 360, 400, 250);
line(540, 360, 700, 250);
line(540, 360, 600, 500);
